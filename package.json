{
  "name": "kash",
  "version": "1.0.3",
  "homepage": "https://github.com/matthewhudson/kash",
  "description": "Fast and simple in-memory caching.",
  "main": "dist/cjs/bundle.js",
  "module": "dist/esm/bundle.js",
  "browser": "dist/browser/bundle.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "keywords": [
    "cache",
    "caching",
    "ram",
    "memory",
    "storage"
  ],
  "author": {
    "name": "Matthew Hudson",
    "email": "matthud@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewhudson/kash.git"
  },
  "bugs": {
    "url": "https://github.com/matthewhudson/kash/issues"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.20.2",
    "babel-jest": "29.5.0",
    "benchmark": "2.1.4",
    "codecov": "3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "jest": "29.5.0",
    "rollup": "2.79.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "standard": "17.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "jest",
    "test:coverage": "jest --coverage && codecov"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
